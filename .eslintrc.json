{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "plugin:react/recommended",
        "eslint:recommended"
    ],
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": 0,
        "no-mixed-spaces-and-tabs": 0,
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-unused-expressions": "warn",
        "no-useless-concat": "warn",
        "block-scoped-var": 0,
        "consistent-return": 0,
        "no-console": 0,
        "react/jsx-key": 0,
        "react/no-unescaped-entities": 0,
        "react/no-direct-mutation-state": 0
    }
}